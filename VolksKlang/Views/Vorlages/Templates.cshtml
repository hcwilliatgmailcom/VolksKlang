@model IEnumerable<VolksKlang.Models.Objekt>

@{
    ViewData["Title"] = "Index";
}

<h1>Ein Objekt aus einer Vorlage erstellen</h1>

 
<table class="table">
 
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Objektbezeichnung)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Beschreibung)
            </td>

  
            <td>
                <a asp-action="Copy" class="btn btn-danger" asp-route-id="@item.ID">Neues Objekt erstellen</a>
            </td>


        </tr>
}
    </tbody>
</table>
